---
import Layout from '../layouts/Layout.astro'
import SobreMi from '../components/SobreMi_es.astro'
import Proyectos from '../components/Proyectos_es.astro'
const lang = 'es'
const title = 'Programador de sitios web accesibles.'
const description =
  'Diseño y programación de páginas web que cumplen con los estándares y son accesibles. Creación de sitios con Astro, WordPress, Prestashop, Joomla, personalizados.'
const canonical = ''
---

<Layout
  lang={lang}
  title={title}
  description={description}
  canonical={canonical}
>
  <SobreMi />
  <Proyectos />
</Layout>
