---
import Layout from '../../layouts/Layout.astro'
import SobreMi from '../../components/SobreMi_en.astro'
import Proyectos from '../../components/Proyectos_en.astro'
const lang = 'en'
const title = 'Accessible website designer and developer.'
const description =
  'I am a web designer and a web developer, specialising in accessible, user-friendly web sites. WordPress, Prestashop, Joomla and custom web site design.'
const canonical = 'en/'
---

<Layout
  lang={lang}
  title={title}
  description={description}
  canonical={canonical}
>
  <SobreMi />
  <Proyectos />
</Layout>
